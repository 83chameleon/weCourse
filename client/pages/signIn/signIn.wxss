/* pages/signIn/signIn.wxss */
.chapter-title{
  font-size: 30rpx;
  padding: 10rpx 0rpx 10rpx 30rpx;
}

.set-time{
  font-size: 32rpx;
}

.last-count{
  float: right;
  margin-right: 30rpx;
}

.teacher-pane,.student-pane{
  width: 100%;
}

.time-title{
  font-size:32rpx;
  padding-top:5rpx;
  float: right;
  margin-right: 40rpx;
  overflow: hidden;
}

.rest-time{
  font-size: 32rpx;
  float: right;
}

.start-test{
  width: 100%;
}

.test{
  width: 100%;
}

.info-rule{
  font-size: 28rpx;
  padding-bottom: 10rpx;
  width:100%;
  margin-left:30rpx;
  color:#E6A23C;
}

.cu-form-group .title {
  min-width: calc(4em + 30rpx);
}

.location-map{
  width:100%;
  height:400rpx;
  border:1rpx solid #909399;
  box-shadow: 0 0 10rpx #B09999;
}

.check-in{
  width: 100%;
  margin: 50rpx 0 0;
}